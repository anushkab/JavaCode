package LABS;

public class FIB {
	 public static int rFib( int n,int m) {
		 if (n == 0 || n == 1)
			 return 1;
		 else

			 return (rFib (n-1,m) + rFib (n-2,m))*Math.abs(m);

	 }
	 public static int iFib( int n,int m) 
	 {
	 if(n==0) return 0;
	 else if(n==1) return 1;
	 else
	 {
		 int a=0;
		 int b=1;
		 for(int i=2;i<n+1;i++)
		 {
			int temp=b;
			b=(a+b)*Math.abs(m);
			a=temp;
		 }
		 return b;

	 }
	 }
	 public static int CleverAlgo( int n,int m) 
	 {
		 int[][] arr=new int[2][2];
		 int[][] aryr={{1,1},{1,0}};
		return 'i';
	 }

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int x=rFib(0,1);
System.out.print(x);
	}

}
